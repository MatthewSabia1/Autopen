# Supabase Configuration
# Your Supabase project URL (found in your Supabase dashboard)
VITE_SUPABASE_URL=https://your-project-id.supabase.co
# Your Supabase anon/public key (found in your Supabase dashboard)
VITE_SUPABASE_ANON_KEY=your-supabase-anon-key
# Your Supabase service role key (for admin access - keep this secure!)
VITE_SUPABASE_SERVICE_ROLE_KEY=your-supabase-service-role-key

# OpenRouter Configuration (AI Model Access)
# Your OpenRouter API key (get one at https://openrouter.ai)
VITE_OPENROUTER_API_KEY=sk-or-v1-your-openrouter-api-key
# The AI model to use (default: deepseek-r1)
VITE_OPENROUTER_MODEL=deepseek/deepseek-r1:free
# OpenRouter API endpoint
VITE_OPENROUTER_API_URL=https://openrouter.ai/api/v1/chat/completions

# OpenAI Configuration (Alternative to OpenRouter)
# Your OpenAI API key (if using OpenAI instead of OpenRouter)
VITE_OPENAI_API_KEY=sk-your-openai-api-key
# OpenAI API endpoint
VITE_OPENAI_API_URL=https://api.openai.com/v1/engines/

# Google Gemini Configuration (Optional)
# Your Gemini API key (if using Gemini features)
VITE_GEMINI_API_KEY=your-gemini-api-key

# Application Configuration
# The URL where your application is running
VITE_APP_URL=http://localhost:5173

# Node Environment
# Set to development for local development, production for production
NODE_ENV=development
